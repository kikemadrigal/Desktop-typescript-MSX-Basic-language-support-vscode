{
    "name": "MSXBasic",
    "displayName": "MSXBasic",
    "description": "MSXBasic lanuguage support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
		"type": "git",
		"url": "https://github.com/kikemadrigal/MSXBasic-language-support-vscode"
    },
    "icon": "images/msxbasic.png",
    "contributes": {
        "languages": [{
            "id": "MSXBasic",
            "aliases": ["MSXBasic", "MSXBasic"],
            "extensions": [".bas",".bad",".asc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "MSXBasic",
            "scopeName": "source.MSXBasic",
            "path": "./syntaxes/MSXBasic.tmLanguage.json"
        }]
    }
}